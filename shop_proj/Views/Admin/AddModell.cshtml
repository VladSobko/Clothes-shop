@model shop_proj.Models.Modell
@{
    ViewBag.Title = "Додавання нової моделі";
}
<h2>@ViewBag.Title</h2>
<form asp-action="addmodell" asp-controller="admin" enctype="multipart/form-data">

    <div class="form-group">
        <label asp-for="CategoryId" class="control-label">№ товару</label>
        <input type="text" asp-for="CategoryId" class="form-control" value="@ViewBag.Id" />
    </div>
    <div class="form-group">
        <label asp-for="Name" class="control-label">Назва</label>
        <input type="text" asp-for="Name" class="form-control" />
    </div>

   
    <div class="form-group">
        <input type="submit" value="Додати" class="btn btn-default" />
    </div>
</form>



